{
  "name": "puppeteerjs",
  "version": "1.0.0",
  "description": "Automated testing with puppeteer",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --timeout=30000 ./tests",
    "test:snapshot": "jest --config=jest.config.js --detectOpenHandles --forceExit",
    "test:snapshot:update": "jest --config=jest.config.js --updateSnapshot --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorFerro/PUPPETEERJS.git"
  },
  "author": "Igor Ferro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IgorFerro/PUPPETEERJS/issues"
  },
  "homepage": "https://github.com/IgorFerro/PUPPETEERJS#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "jest": "^24.9.0",
    "jest-image-snapshot": "^2.12.0",
    "mocha": "^7.0.0",
    "puppeteer": "^2.0.0"
  },
  "devDependencies": {
    "prettier": "1.19.1"
  }
}
